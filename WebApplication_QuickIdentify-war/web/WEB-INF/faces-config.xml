    <managed-bean>
        <managed-bean-name>page1</managed-bean-name>
        <managed-bean-class>controllers.Redirection</managed-bean-class>
    
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>sessionController2</managed-bean-name>
        <managed-bean-class>controllers.SessionController2</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>sessionsController</managed-bean-name>
        <managed-bean-class>controllers.SessionsController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
